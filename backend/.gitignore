# Node.js specific ignores
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Environment Variables
.env
.env.GEMINI_API_KEY
.env.local
.env.*.local
.env.development.local
.env.test.local
.env.production.local


# Build artifacts (if you have a build step for your backend, e.g., TypeScript compilation)
/dist/
/build/
/out/

# MongoDB specific ignores
# Default MongoDB data directory (if running locally without explicit path)
/data/db/
# MongoDB journal files (for recovery, often in data/db or logs)
/journal/

# Database dump files (if you create backups locally)
*.bak
*.bson
*.json # Be careful if you have legitimate JSON files to track
dump/ # Common directory for mongodump output
mongodump/

# Log files
logs/
*.log
npm-debug.log* # Redundant but harmless if already above

# IDE and Editor Specific
.vscode/
!.vscode/extensions.json # Keep recommended extensions
.idea/
.DS_Store # macOS specific
Thumbs.db # Windows specific
ehthumbs.db # Windows specific
.history/ # VS Code Local History
*.sublime-project
*.sublime-workspace
.project
.directory
.classpath

# Compiled/temporary files
*.tmp
*.bak
*.swp # Vim swap files
*~ # Emacs backup files
.#* # Emacs auto-save files

# Test/Coverage reports
/coverage/
.nyc_output/